<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <style name="ScrollScreen">
    <item name="android:orientation">vertical</item>
  </style>
  <style name="RectangleBox">
    <item name="android:background">@drawable/ic_rectangle</item>
    <item name="android:layout_centerHorizontal">true</item>
  </style>
  <style name="CheckmarkText" parent="PrimaryText">
    <item name="android:paddingRight">10dp</item>
    <item name="android:importantForAccessibility">yes</item>   
    <item name="android:layout_marginBottom">16dp</item>       
  </style>
  <style name="CheckmarkContainer">
    <item name="android:importantForAccessibility">no</item>    
  </style>
  <style name="Checkmark" parent="PrimaryText">
    <item name="android:importantForAccessibility">no</item>
    <item name="android:clickable">false</item>
    <item name="android:background">#0000</item>
    <item name="android:layout_marginTop">3dp</item>
    <item name="android:paddingRight">10dp</item>
  </style>
  <style name="HeaderText" parent="PrimaryTextBold">
    <item name="android:textSize">24sp</item>
  </style>
  <style name="SwitchTextStyle" parent="ExplanationTextHeader">
        <item name="android:textColor">#000000</item>
  </style>
  <style name="SwitchPlaneStyle"> 
        <item name="android:checked">false</item>
        <item name="android:textOff">Nej</item>
        <item name="android:textOn">Ja</item>
        <item name="android:showText">true</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_alignParentRight">true</item>
  </style>
	<style name="Footer" parent="PrimaryText">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">@color/primaryText</item>
	</style>
	<style name="WhatIsNewFooter" parent="PrimaryText">
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/primaryText</item>
	</style>
</resources>